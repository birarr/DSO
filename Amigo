package br.ufsc.ine5605.exe2;

/**
 *
 * @author 00893917052
 */
public class Amigo {
    private String nome;
    private String fone;
    private String email;
    
    public Amigo() {
    }    
    
public Amigo(String nome, String fone, String email) {
    this.nome = nome;
    this.fone = fone;
    this.email = email;
}

public String getNome() {
    return this.nome;
}

public void setNome(String nome) {
    this.nome = nome;
}

public String getFone() {
    return this.fone;
}

public void setFone(String fone) {
    this.fone = fone;
}

public String getEmail() {
    return this.email;
}

public void setEmail(String email) {
    this.email = email;
}

}
